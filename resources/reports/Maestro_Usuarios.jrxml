	<queryString>
		<![CDATA[select
    idUsuario,
    u.nombre,
    r.nombre as Rol,
    email,
    telefono,
    (CASE
        WHEN habilitado = 1 THEN 'Sí'
        ELSE 'No'
    END) as Habilitado
from
    usuario u,
    rol r
where
    u.idRol = r.idRol
order by habilitado desc]]>
	</queryString>
