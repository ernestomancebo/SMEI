	<parameter name="periodo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    r.idreservacion,
    u.nombre as usuario,
    date_format(r.fechainicio, '%e-%m-%Y') as fecha,
    concat_ws(' - ',
            date_format(r.fechainicio, '%h:%i %p'),
            date_format(r.fechafin, '%h:%i %p')) as horario,
    e.nombre as lugar,
    r.cantidaddepersonas,
    r.descripcion,
    es.nombre as estado
FROM
    reservaciones r,
    espacios e,
    usuario u,
    estados_reservaciones es
WHERE
    u.idusuario = r.idusuario
        AND e.idespacio = r.idespacio
        AND es.idestado = r.idestado
        AND es.idestado in ($P{idEstado1}, $P{idEstado2}, $P{idEstado3})
        AND r.fechaInicio >= date_sub(curdate(), interval $P{periodo} month)
order by es.idEstado]]>
	</queryString>
